{
  "name": "vue-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "cross-env CLIENT_PORT=7070 vue-cli-service serve",
    "dev:server": "cross-env NODE_ENV=development nodemon server/index.js",
    "dev": "npm-run-all -p dev:**",
    "build:server": "cross-env VUE_ENV=server vue-cli-service build --no-clean",
    "build:client": "vue-cli-service build",
    "build": "npm-run-all -p build:**",
    "start": "cross-env NODE_ENV=production nodemon server/index.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "axios": "^0.21.1",
    "babel-eslint": "^10.0.1",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": ">=6",
    "koa": "^2.13.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": ">=10",
    "memory-fs": "^0.5.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "style-resources-loader": "^1.4.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "/src/**": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
